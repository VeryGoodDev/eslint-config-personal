{
  "name": "eslint-config-personal",
  "version": "1.0.0",
  "description": "Personal preferences for eslint",
  "main": "index.js",
  "repository": "https://github.com/verygooddev/eslint-config-personal",
  "author": "VeryGoodDev <very_good_dev@outlook.com>",
  "license": "MIT",
  "exports": {
    ".": "./index.js",
    "./browser-js": "./configs/browser-js.js",
    "./browser-ts": "./configs/browser-ts.js",
    "./jest-js": "./configs/jest-js.js",
    "./jest-ts": "./configs/jest-ts.js",
    "./node-js": "./configs/node-js.js",
    "./node-ts": "./configs/node-ts.js",
    "./preact-js": "./configs/preact-js.js",
    "./preact-ts": "./configs/preact-ts.js",
    "./react-js": "./configs/react-js.js",
    "./react-ts": "./configs/react-ts.js"
  },
  "scripts": {
    "lint": "eslint index.js"
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/eslint-plugin": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "prettier": "^2.0.2"
  },
  "devDependencies": {
    "eslint": "^8.9.0"
  },
  "peerDependencies": {
    "eslint": "^8.9.0"
  },
  "eslintConfig": {
    "extends": [
      "./index.js"
    ],
    "rules": {
      "id-length": "off",
      "max-lines": "off",
      "no-magic-numbers": "off",
      "sort-keys": "off"
    }
  }
}
